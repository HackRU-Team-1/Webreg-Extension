{
  "manifest_version": 2,

  "name": "Five Point Oh - A Webreg Extension",
  "description": "Top Secret",
  "icons": {"128": "logo.png"},
  "version": "1.0",
  "permissions": [
        "tabs", "<all_urls>"
  ],
  "content_scripts": [
    {
	  "matches": [ "https://sims.rutgers.edu/*", "https://sis.rutgers.edu/*"],
      
      "css": ["stylesheet.css"],
      "js": ["contentscript.js"],
	  "run_at": "document_end"
    }
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
